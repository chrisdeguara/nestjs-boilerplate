{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest-project/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nest-project/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nest-project",
  "projects": {
    "nest-project": {
      "type": "application",
      "root": "apps/nest-project",
      "entryFile": "main",
      "sourceRoot": "apps/nest-project/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-project/tsconfig.app.json"
      }
    },
    "fx-app": {
      "type": "application",
      "root": "apps/fx-app",
      "entryFile": "main",
      "sourceRoot": "apps/fx-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/fx-app/tsconfig.app.json"
      }
    },
    "fx-library": {
      "type": "library",
      "root": "libs/fx-library",
      "entryFile": "index",
      "sourceRoot": "libs/fx-library/src",
      "compilerOptions": {
        "tsConfigPath": "libs/fx-library/tsconfig.lib.json"
      }
    },
    "custom-logger": {
      "type": "library",
      "root": "libs/custom-logger",
      "entryFile": "index",
      "sourceRoot": "libs/custom-logger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/custom-logger/tsconfig.lib.json"
      }
    },
    "redis-cache": {
      "type": "library",
      "root": "libs/redis-cache",
      "entryFile": "index",
      "sourceRoot": "libs/redis-cache/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis-cache/tsconfig.lib.json"
      }
    },
    "microservice-example": {
      "type": "application",
      "root": "apps/microservice-example",
      "entryFile": "main",
      "sourceRoot": "apps/microservice-example/src",
      "compilerOptions": {
        "tsConfigPath": "apps/microservice-example/tsconfig.app.json"
      }
    },
    "calculator": {
      "type": "library",
      "root": "libs/calculator",
      "entryFile": "index",
      "sourceRoot": "libs/calculator/src",
      "compilerOptions": {
        "tsConfigPath": "libs/calculator/tsconfig.lib.json"
      }
    },
    "calculator-app": {
      "type": "application",
      "root": "apps/calculator-app",
      "entryFile": "main",
      "sourceRoot": "apps/calculator-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/calculator-app/tsconfig.app.json"
      }
    },
    "user-management": {
      "type": "application",
      "root": "apps/user-management",
      "entryFile": "main",
      "sourceRoot": "apps/user-management/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-management/tsconfig.app.json"
      }
    }
  }
}