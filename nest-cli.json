{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest-project/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nest-project/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nest-project",
  "projects": {
    "nest-project": {
      "type": "application",
      "root": "apps/nest-project",
      "entryFile": "main",
      "sourceRoot": "apps/nest-project/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-project/tsconfig.app.json"
      }
    },
    "fx-app": {
      "type": "application",
      "root": "apps/fx-app",
      "entryFile": "main",
      "sourceRoot": "apps/fx-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/fx-app/tsconfig.app.json"
      }
    }
  }
}